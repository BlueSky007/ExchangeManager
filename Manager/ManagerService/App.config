<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="ManagerDBConnection" value="data source=.;initial catalog=ManagerDB;user id=sa;password=Omni1234;Connect Timeout=60"/>
  </appSettings>

  <system.diagnostics>
    <sources>
      <source name="Logger" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="eventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="Manager" traceOutputOptions="DateTime,ThreadId"/>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="sourceSwitch" value="Information"/>
    </switches>
    <trace autoflush="true"/>
  </system.diagnostics>

<startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
</startup>
  <system.web>
    <compilation debug="true" />
  </system.web>

</configuration>
